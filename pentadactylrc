" Passkeys for gmail
set passkeys+=mail.google.com:'!#*+-./<C-Down><C-s><Enter>?INU[]acefjklmnopqrsuvxyz',*a,*n,*r,*s,*t,*u,ga,gc,gd,gi,gl,gs,gt,yo

" Always open help in a new tab
set newtab+=help

set mapleader=','

" Browser back and forward
nmap -builtin h H
nmap -builtin H h
nmap -builtin l L
nmap -builtin L l


" vim: set ft=vim
